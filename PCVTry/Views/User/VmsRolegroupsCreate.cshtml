@model PCVTry.Models.VmsRolegroup

@{
    ViewData["Title"] = "VmsRolegroupsCreate";
    Layout = "_Layout";
}

<!DOCTYPE html>




<html>


<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <style>
        body {
            font-family: "Lato", sans-serif;
        }

        .sidenav {
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: #1B295B;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 60px;
        }

            .sidenav a {
                padding: 8px 8px 8px 32px;
                text-decoration: none;
                font-size: 15px;
                color: #f1f1f1;
                display: block;
                transition: 0.3s;
            }

                .sidenav a:hover {
                    color: #818181;
                }

            .sidenav .closebtn {
                position: absolute;
                top: 0;
                right: 25px;
                font-size: 25px;
                margin-left: 50px;
            }

       @@media screen and (max-height: 450px) {
            .sidenav





        {
            padding-top: 10px;
        }

        .sidenav a {
            font-size: 10px;
        }
        }
    </style>

</head>

<body>
    <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <a href="/User/RequestInfoesCreate">Create</a>
        <a href="/User/UserTablesCreate">User Information</a>
        <a href="/User/UserRequirementsCreate">Requirements</a>
        <a href="/User/VmsRolegroupsCreate">Application Access (VMS Role/Group)</a>
        <a href="/User/OtsAccessCreate">Application Access (OTS Access)</a>
        <a href="/User/BpoRoleGroupsCreate">Application Access (BPO Role/Group)</a>
        <a href="/User/LrvRoleGroupCreate">Application Access (LRV Role/Group)</a>
    </div>




    <span style="font-size:20px;cursor:pointer" onclick="openNav()">&#9776; Menu</span>

    <script>
function openNav() {
            document.getElementById("mySidenav").style.width = "250px";
        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";
        }</script> <br>

</body>

</html>

<h2>User Request Form</h2>

<h4>VMS Role/Group</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="VmsRolegroupsCreate">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
           
            <div class="form-group">
                <label class="control-label">Accounting Group</label>
                @Html.EditorFor(x => x.AccountGroup)
            </div>
            <div class="form-group">
                <label  class="control-label">Admin Support</label>
                @Html.EditorFor(x => x.AdminSupport)
            </div>
            <div class="form-group">
                <label class="control-label">Assignor</label>
                @Html.EditorFor(x => x.Assignor)
            </div>
            <div class="form-group">
                <label class="control-label">Auditor</label>
                @Html.EditorFor(x => x.Auditor)
            </div>
            <div class="form-group">
                <label class="control-label">Client Services Department Manager</label>
                @Html.EditorFor(x => x.CliServiDeptMgr)
            </div>
            <div class="form-group">
                <label class="control-label">Client Servies Group</label>
                @Html.EditorFor(x => x.CliServiGroup)
            </div>
            <div class="form-group">
                <label class="control-label">Commercial Group</label>
                @Html.EditorFor(x => x.CommercialGroup)
            </div>
            <div class="form-group">
                <label class="control-label">Contractor QC</label>
                @Html.EditorFor(x => x.ContractorQc)
            </div>
            <div class="form-group">
                <label class="control-label">IT Group</label>
                @Html.EditorFor(x => x.Itgroup)
            </div>
            <div class="form-group">
                <label class="control-label">Processing/Shipping Group</label>
                @Html.EditorFor(x => x.ProcessingShippingGroup)
            </div>
            <div class="form-group">
                <label class="control-label">QC Supervisor Lead</label>
                @Html.EditorFor(x => x.QcsupervisorLead)
            </div>
            <div class="form-group">
                <label class="control-label">QC</label>
                @Html.EditorFor(x => x.Qc)
            </div>
            <div class="form-group">
                <label class="control-label">Sales Group</label>
                @Html.EditorFor(x => x.SalesGroup)
            </div>
            <div class="form-group">
                <label class="control-label">Status Group</label>
                @Html.EditorFor(x => x.StatusGroup)
            </div>
            <div class="form-group">
                <label class="control-label">Vendor Relations Group</label>
                @Html.EditorFor(x => x.VendorRelationsGroup)
            </div>
            <div class="form-group">
                <input type="submit" value="Next" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
